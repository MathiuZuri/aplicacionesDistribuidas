{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Port=3306;Database=ms_cliente_db;Uid=root;Pwd=;"
  },
  "Swagger": {
    "Enabled": true,
    "RoutePrefix": "swagger"
  },
  "ServiceSettings": {
    "ServiceName": "ms-cliente-service"
  },
  "AllowedHosts": "*",
  "Eureka": {
    "Client": {
      "ServiceUrl": "http://localhost:8090/eureka/", 
      "ShouldRegisterWithEureka": true,           
      "ShouldFetchRegistry": true               
    },
    "Instance": {
      "AppName": "MS-CLIENTE-SERVICE",
      "InstanceId": "${spring:application:name}:${spring:application:instance_id:${random.value}}",
      "PreferIpAddress": false,
      "Hostname": "ms-cliente-service"
    }
  },
  "spring": {
    "application": {
      "name": "ms-cliente-service"
    }
    
}
}